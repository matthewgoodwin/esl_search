<%= simple_form_for([ consultation, review ], remote: true) do |f| %>
  <!-- add appoinment reservation number here -->
  <%= f.input :review_title, label: false, placeholder: "title" %>
  <%= f.input :comment, label:false, placeholder: "comments", as: :text %>
  <%= f.input :star, label:false, prompt: "stars", collection: (1..5) %>
  <%= f.button :submit, class: "btn btn-primary" %>
<% end %>
